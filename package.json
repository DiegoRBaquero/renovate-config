  "renovate-config": {
    "default": {
      "extends": [
        "group:all",
        "config:base", 
        ":semanticCommitTypeAll(chore)"
      ],
      "docker": {
        "enabled": false
      },
      "pinVersions": false,
      "groupName": "all",
      "packageRules": [
        {
          "packageNames": [
            "hapi"
          ],
          "allowedVersions": "<17"
        },
        {
          "packageNames": [
            "hoek", "rejoice", "scooter"
          ],
          "allowedVersions": "<5"
        },
        {
          "packageNames": [
            "good"
          ],
          "allowedVersions": "<8"
        },
        {
          "packageNames": [
            "lab"
          ],
          "allowedVersions": "<15"
        },
        {
          "packageNames": [
            "mongoose"
          ],
          "allowedVersions": "<5"
        }
      ]
    }
  } 
}
